        {
          "name": "prothetic_user",
          "description": "Prothetic user role with report access"
        }
      ]
    },
    "requiredCredentials": ["password", "otp"],
    "otpPolicyType": "totp",
    "otpPolicyAlgorithm": "HmacSHA1",
    "otpPolicyInitialCounter": 0,
    "otpPolicyDigits": 6,
    "otpPolicyLookAheadWindow": 1,
    "otpPolicyPeriod": 30,
    "otpSupportedApplications": ["totpAppGoogleName", "totpAppFreeOTPName"],
