{
    "realm": "reports-realm",
    "enabled": true,
    "accessTokenLifespan": 120,
    "accessTokenLifespanForImplicitFlow": 120,
    "ssoSessionIdleTimeout": 7200,
    "ssoSessionMaxLifespan": 7200,
    "accessCodeLifespan": 60,
    "accessCodeLifespanUserAction": 300,
    "roles": {
      "realm": [
        {
